<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{ active }}" bind:change="onChange" active-color="#07c160">
  <van-tabbar-item icon="home-o">
    <image slot="icon" src="{{ icon.normal }}" mode="aspectFit" style="width: 40px; height: 18px;" />
    <image slot="icon-active" src="{{ icon.active }}" mode="aspectFit" style="width: 40px; height: 18px;" /> 资质申请
  </van-tabbar-item>

  <van-tabbar-item icon="fire-o">
    <image slot="icon" src="{{ icon.normal }}" mode="aspectFit" style="width: 40px; height: 18px;" />
    <image slot="icon-active" src="{{ icon.active }}" mode="aspectFit" style="width: 40px; height: 18px;" /> 预约选摊
  </van-tabbar-item>
  <van-tabbar-item icon="eye-o">
    <image slot="icon" src="{{ icon.normal }}" mode="aspectFit" style="width: 40px; height: 18px;" />
    <image slot="icon-active" src="{{ icon.active }}" mode="aspectFit" style="width: 40px; height: 18px;" /> 智能选摊
  </van-tabbar-item>
  <van-tabbar-item icon="manager-o">
    <image slot="icon" src="{{ icon.normal }}" mode="aspectFit" style="width: 40px; height: 18px;" />
    <image slot="icon-active" src="{{ icon.active }}" mode="aspectFit" style="width: 40px; height: 18px;" /> 我的
  </van-tabbar-item>
</van-tabbar>